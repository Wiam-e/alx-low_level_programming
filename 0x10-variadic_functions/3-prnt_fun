#inslude "prnt_fun.h"
#include "variadic_functions.h"
#include <stdio.h>
#include <stdarg.h>
/**
* prnt_i - function that prints ints
* @i: va_list type
* Return: Success
*/
void prnt_i(va_list i)
{
printf("%d", va_arg(i, int));
}

/**
* prnt_c - function that prints a char
* @c: va_list type
* Return: Success
*/
void prnt_c(va_list c)
{
printf("%c", va_arg(c, int));
}

/**
* prnt_flt - function that prints floats
* @f: va_list type
* Return:Success
*/
void prnt_flt(va_list f)
{
printf("%f", va_arg(f, double));
}

/**
* prnt_s - function to print string
* @a: va_list type
* Return: Success
*/
void prnt_s(va_list a)
{
char *s;
s = va_arg(a, char*);
if (s == NULL)
{
printf("(nil)");
}
printf("%s", s);
}